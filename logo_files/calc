var calc={getDegree:function(n){var t=Math.round(n*180/Math.PI)-90;return this.fixDegree(t)},getRadianFromDegree:function(n){return n*(Math.PI/180)},valueInRange:function(n,t,i){if(t>i){var r=t;t=i;i=t}return n>=t&&n<=i},getRadian:function(n,t,i,r){var u=n-i,f=t-r;return Math.atan2(f,u)},fixDegree:function(n){var t=n;return t<0&&(t+=360),t>360&&(t%=360),t},randomInterval:function(n,t){return Math.floor(Math.random()*(t-n+1)+n)},rotatePoint:function(n,t,i,r,u){var f=u*Math.PI/180;return{x:parseFloat(Math.cos(-f)*(n-i)-Math.sin(-f)*(t-r)+i),y:parseFloat(Math.sin(-f)*(n-i)+Math.cos(-f)*(t-r)+r)}},rotatePointF:function(n,t,i,r,u){var f=u*Math.PI/180;return{x:(Math.cos(-f)*(n-i)-Math.sin(-f)*(t-r)+i).toFixed(2),y:(Math.sin(-f)*(n-i)+Math.cos(-f)*(t-r)+r).toFixed(2)}},getDistance:function(n,t,i,r){return Math.sqrt(Math.pow(n-i,2)+Math.pow(t-r,2))},getAngleOfPoints:function(n,t,i,r){var u=i-n,f=r-t;return Math.atan2(f,u)*(180/Math.PI)}}