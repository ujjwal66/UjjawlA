Textformat=function(n){var t=this;t.bold=!1;t.italic=!1;t.text="New Text";t.font=Textformat.defaultFont;t.size=100;t.alignment="center";t.letterspacing=0;n!=undefined&&(t.text=n);t.clone=function(){var n=new Textformat;return n.bold=t.bold,n.italic=t.italic,n.text=t.text,n.font=t.font,n.size=t.size,n.alignment=t.alignment,n.letterspacing=t.letterspacing||0,n}};Textformat.fromObj=function(n){for(var r=new Textformat(n.text),u=Object.keys(n),t,i=0,f=u.length;i<f;i++)t=u[i],typeof r[t]!="undefined"&&(r[t]=n[t]);return r};Textformat.defaultFont="Fjalla One";Textformat.prototype.getFontFileName=function(){var n=this.font,t;return n||(n=Textformat.defaultFont),t=n.indexOf(","),t>=0&&(n=n.substr(0,n.indexOf(","))),n=n.replace(/[^A-Za-z0-9_]+|\s+/gi,"").toLowerCase(),this.bold&&this.italic?n+="-bold-italic":this.italic?n+="-italic":this.bold&&(n+="-bold"),n}