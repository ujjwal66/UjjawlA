Messagebox=function(n,t,i,r,u,f,e,o,s,h,c,l){var a=this;a.drawningboard=n;a.drawningboard.messagemenu=a;a.isOpen=!1;a.overlay=typeof e=="undefined"?!1:e;a.content=!1;typeof o!="undefined"&&(a.content=o);a.cssclass=typeof s!="undefined"?s:"";a.confirm=function(){a.hide();a.isOpen=!1;i&&i()};a.registerevents=function(){r||a.dom.find(".confirm-td").hide();u||a.dom.find(".cancel-td").hide();$("#logobody").append(a.dom);$("#partnersbody").append(a.dom);a.dom.find(".messagebox").center(h,c);a.dom.find(".confirm").on("click",function(){a.confirm()});a.dom.find(".cancel").on("click",function(){a.hide();a.isOpen=!1;l&&l()});a.dom.find(".close").on("click",function(){a.hide();a.isOpen=!1;l&&l()})};a.show=function(){var n='<div class="messagewrapper"><div class="messagebox '+s+'"><span class="close">+<\/span><div class="message">'+t+"<\/div>";a.content&&(n+=a.content);n+='<div class="buttombar" > <table><tr><td class="cancel-td"><div class="multi cancel">'+u+'<\/div><\/td><td class="confirm-td"><div class="irreversible confirm">'+r+"<\/div><\/td><\/tr><\/table><\/div><\/div><\/div>";a.dom=$(n);a.registerevents();$("html").css("overflow-y","hidden");a.overlay&&($(".overlay").remove(),$("body").prepend('<div class="overlay" style="width:100%; height: 100%; background-color: rgba(0, 0, 0, 0.4); position: fixed; left: 0; top: 0; bottom:0; z-index:2;"><\/div>'));a.dom.find(".messagebox").hide().slideDown("fast",function(){a.isOpen=!0})};a.hide=function(){a.dom.find(".messagebox").slideUp("fast",function(){a.dom.remove()});$("html").css("overflow-y","auto");a.overlay&&$(".overlay").remove();f&&f();a.drawningboard.messagemenu=undefined}}