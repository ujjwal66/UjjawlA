function isIE(){var n=window.navigator.userAgent,t=n.indexOf("MSIE ");return t>0||!!navigator.userAgent.match(/Trident.*rv\:11\./)}function arrayUnique(n){for(var t=n.concat(),r,i=0;i<t.length;++i)for(r=i+1;r<t.length;++r)t[i]===t[r]&&t.splice(r--,1);return t}function validateEmail(n){var t=!0;return n.indexOf(".")==-1&&(t=!1),n.indexOf("@")==-1&&(t=!1),t}function getQueryVariable(n){for(var u=window.location.search.substring(1),r=u.split("&"),i,t=0;t<r.length;t++)if(i=r[t].split("="),i[0]==n)return i[1];return!1}function shuffleArray(n){for(var t=n.length,r,i;0!==t;)i=Math.floor(Math.random()*t),t-=1,r=n[t],n[t]=n[i],n[i]=r;return n}function debounce(n,t){var i=null;return function(){var r=this,u=arguments;clearTimeout(i);i=setTimeout(function(){n.apply(r,u)},t)}}function animationFrame(n){return function(){var t=this,i=arguments;requestAnimationFrame(function(){n.apply(t,i)})}}function isLocalStorageAvailable(){if(window._isLocalStorageAvailable!==null)return window._isLocalStorageAvailable;try{localStorage.setItem("__localstoragetest","1");localStorage.removeItem("__localstoragetest");window._isLocalStorageAvailable=!0}catch(n){window._isLocalStorageAvailable=!1}return window._isLocalStorageAvailable}function getBlogFormattedDate(n){var r=new Date(n),u=r.getFullYear().toString(),t=(r.getMonth()+1).toString(),i=r.getDate().toString();return i.length<2&&(i="0"+i),t.length<2&&(t="0"+t),t+"."+i+"."+u}jQuery.fn.center=function(n,t){var i=$(this).outerWidth(),r=$(this).outerHeight();return n!==null&&typeof n!="undefined"&&(i=n),t!==null&&typeof t!="undefined"&&(r=t),this.css("position","absolute"),this.css("top",Math.max(0,($(window).height()-r)/2)),this.css("left",Math.max(0,($("#logomaker").width()-i)/2)),this};var requestAnimationFrame=window.requestAnimationFrame||function(){var n=0;return window.webkitRequestAnimationFrame||window.mozRequestAnimationFrame||function(t){var r=(new Date).getTime(),i;return i=Math.max(0,16-(r-n)),n=r+i,setTimeout(function(){t(r+i)},i)}}();jQuery.fn.nodoubletapzoom=function(){$(this).bind("touchstart",function(n){var t=n.timeStamp,r=$(this).data("lastTouch")||t,i=t-r,u=n.originalEvent.touches.length;($(this).data("lastTouch",t),!i||i>500||u>1)||(n.preventDefault(),$(this).trigger("click").trigger("click"))})};jQuery.attrHooks.startoffset={set:function(n,t){return n.setAttributeNS(null,"startOffset",t+""),t}};jQuery.fn.outerHTML=function(){return jQuery("<div />").append(this.eq(0).clone()).html()};jQuery.touchSupport=function(){var t=/*@cc_on!@*/!1||!!document.documentMode,i=/Edge\/\d./i.test(navigator.userAgent),n=!1;return i?(navigator.maxTouchPoints>0||navigator.msMaxTouchPoints>0)&&(n=!0):("ontouchstart"in window||"onmsgesturechange"in window&&!t)&&(n=!0),n};RegExp.escape=function(n){return n.replace(/[-[\]{}()*+?.,\\^$|#\s]/g,"\\$&")};String.prototype.replaceAll=function(n,t){return this.replace(new RegExp(RegExp.escape(n),"g"),t)};window._isLocalStorageAvailable=null